{
  "name": "@cloudflare/stagehand",
  "version": "0.0.1-next",
  "scripts": {
    "build-types": "tsc --emitDeclarationOnly --outDir dist/types",
    "gen-stagehand-code": "cd .. && pnpm i --ignore-scripts && pnpm run gen-version && pnpm run build-dom-scripts",
    "build": "npm run gen-stagehand-code && npx vite build"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "types": "./dist/types/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/lib/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./llm/*": {
      "types": "./dist/types/lib/llm/*.d.ts",
      "require": "./dist/cjs/llm/*.js",
      "import": "./dist/esm/llm/*.js",
      "default": "./dist/cjs/llm/*.js"
    }
  },
  "devDependencies": {
    "@browserbasehq/sdk": "^2.6.0",
    "@types/node": "^22.17.2",
    "pkg-pr-new": "^0.0.57",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "@cloudflare/playwright": "^0.0.11",
    "ai": "^4.3.16"
  },
  "peerDependencies": {
    "zod": "^3.25.48"
  }
}
